{
    "name": "alcasol/mvc_app",
    "description": "Proyecto MVC para APP WEB corriendo en XAMPP y PHP",
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "restart": "sudo /Applications/XAMPP/xamppfiles/xampp restart"
    },
    "require": {
        "guzzlehttp/guzzle": "^7.8",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Core\\": "core/",
            "Utils\\": "utils/"
   
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
